	<queryString><![CDATA[SELECT
     p._codigo as pedido,
     u._nome as cliente,
     t._descricao as statuspedido,
     f._descricao as formapagamento,
     p._datapedido as datapedido,
     p._totalpedido as total,
     p._datapagamento as datapagamento
FROM
      pedidodesktopwap as p, usuario as u, tipoformapagamento 
	as f, tipostatuspedido as t
